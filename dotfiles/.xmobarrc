Config { overrideRedirect = False
       -- , font     = "xft:iosevka-14"
       -- , font     = "xft:Hack Nerd Font 16" -- does not work
       -- , font     = "xft:Mononoki Nerd Font Mono:size=14"
       , font     = "xft:Hack Nerd Font Mono:size=14"
       , additionalFonts = ["xft:Mononoki Nerd Font Mono:size=20"]
       , bgColor  = "#5f5f5f"
       , fgColor  = "#f8f8f2"
       , position = TopW L 90
       , commands = [ Run Weather "EGPF"
                        [ "--template", "<weather> <tempC>Â°C"
                        , "-L", "0"
                        , "-H", "25"
                        , "--low"   , "lightblue"
                        , "--normal", "#f8f8f2"
                        , "--high"  , "red"
                        ] 36000
                    , Run Cpu
                        [ "-L", "3"
                        , "-H", "50"
                        , "--high"  , "red"
                        , "--normal", "green"
                        ] 10
                    , Run Battery [
                        "-t", "<acstatus>: <left>% - <timeleft>",
                        "--",
                        --"-c", "charge_full",
                        "--lows"   , "<fn=1>\62020</fn>  ",
                        "--mediums", "<fn=1>\62018</fn>  ",
                        "--highs"  , "<fn=1>\xf240</fn>  ",
                        "-O", "AC",
                        "-i", "AC",
                        "-o", "Bat",
                        "-h", "green",
                        "-l", "red"
                        ] 10
                    , Run Alsa "default" "Master"
                        [ "--template", "<volumestatus>"
                        , "--suffix"  , "True"
                        , "--"
                        , "--on", ""
                        ]
                    , Run Memory ["--template", "Mem: <usedratio>%"] 10
                    , Run Swap [] 10
                    , Run Com "setxkbmap -print | grep xkb_symbols | awk -F\"+\" '{print $2}'"  [] "keyboard" 30
                    , Run Date "%a %Y-%m-%d <fc=#8be9fd>%H:%M</fc>" "date" 10
                    , Run XMonadLog
                    ]
       , sepChar  = "%"
       , alignSep = "}{"
       -- , template = "%XMonadLog% }{ %alsa:default:Master% | %cpu% | %memory% * %swap% | %EGPF% | %date% "
       , template = "%XMonadLog% }{ %alsa:default:Master% | %battery% |  %date% "
       }
